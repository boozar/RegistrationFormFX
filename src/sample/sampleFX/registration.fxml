        <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
    </rowConstraints>
    <children>
        <TextField fx:id="txtRegLogin" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="31.0" prefWidth="210.0" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="2" />
        <TextField fx:id="txtRegPassword" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="3" />
          <Button fx:id="btn_RegisterFinish" alignment="CENTER" mnemonicParsing="false" onAction="#btnRegistrationAction" prefHeight="31.0" prefWidth="110.0" text="Register" GridPane.columnIndex="2" GridPane.rowIndex="4">
